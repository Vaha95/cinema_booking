{% extends 'base.html.twig' %}

{% block title %}Hello SessionController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 95%; font: 18px/1.5 sans-serif; }
    .uk-card span { font: 12px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <div class="uk-card uk-card-default uk-card-body uk-width-1-2@l">
        <h3 class="uk-card-title">{{ title }}</h3>
        <p>{{ description }}</p>
    </div>
</div>
{% endblock %}
